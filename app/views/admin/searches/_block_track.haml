%tr
  %td
    ID: #{block_track.id}
  %td= check_box_tag("track_#{block_track.id}", value = "#{block_track.id}")
  %td= link_to block_track.title, track_url(:id => block_track.id)
  %td
    author: #{block_track.author}
  %td
    bitrate: #{block_track.bitrate}
  %td
    dimension: #{block_track.dimension}
  %td
    state: #{block_track.state}
  %td= link_to 'Удалить', admin_track_url(block_track.id), :method => :delete
  %td= link_to 'Редактировать', edit_admin_track_url(block_track.id)
