<h1>Editing track</h1>

<% form_for(@track) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :author %><br />
    <%= f.text_field :author %>
  </p>
  <p>
    <%= f.label :bitrate %><br />
    <%= f.text_field :bitrate %>
  </p>
  <p>
    <%= f.label :dimension %><br />
    <%= f.text_field :dimension %>
  </p>
  <p>
    <%= f.label :playlist_id %><br />
    <%= f.text_field :playlist_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @track %> |
<%= link_to 'Back', tracks_path %>